{
    "manifest_version": 3,
    "name": "LeetCode Playground",
    "version": "1.0",
    "description": "Test your code snippets inline on LeetCode",
    "icons": {
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://leetcode.com/*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": ["storage", "activeTab", "scripting"]
  }